{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "^/static/(.*)",
      "headers": {
        "cache-control": "max-age=31536000,immutable"
      }
    },
    {
      "src": "^/(.*)",
      "dest": "/index.html",
      "continue": true
    }
  ],
  "rewrites": [
    { "source": "/admin/(.*)", "destination": "/index.html" },
    { "source": "/admin", "destination": "/index.html" }
  ]
} 
 
 
 